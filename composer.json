{
    "name": "medcenter24/md-core",
    "description": "Backend for the MedCenter24 project",
    "keywords": ["api", "medcenter24", "backoffice", "medicine"],
    "license": "GPL-2.0-only",
    "minimum-stability": "dev",
    "type": "medcenter24-extension",
    "extra": {
        "medcenter24-extension-name": "mcCore"
    },
    "require": {
        "ext-json": "*",
        "php": ">=7.1.3",
        "backup-manager/laravel": "dev-master",
        "barryvdh/laravel-cors": "0.11.3",
        "cmgmyr/messenger": "~2.17",
        "dingo/api": "~2.2.3",
        "intervention/image": "^2.4",
        "irazasyed/telegram-bot-sdk": "3.0.*",
        "laravel/framework": "5.8.*",
        "laravel/tinker": "~1.0",
        "maatwebsite/excel": "~3.1.11",
        "mpdf/mpdf": "~8.0.0",
        "medcenter24/lib-installer": "~1.0",
        "spatie/laravel-medialibrary": "~7.6.0",
        "srmklive/flysystem-dropbox-v2": "dev-master",
        "tymon/jwt-auth": "1.0.0-rc.4.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "~3.2.3",
        "barryvdh/laravel-ide-helper": "^2.6.2",
        "doctrine/dbal": "^2.9.2",
        "fzaninotto/faker": "~1.8",
        "mockery/mockery": "1.2.*",
        "nunomaduro/collision": "^3.0.1",
        "phpunit/phpunit": "~7.5",
        "nwidart/laravel-modules": "5.0.0",
        "roave/security-advisories": "dev-master"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "medcenter24\\mcCore\\App\\": "app",
            "medcenter24\\mcCore\\Tests\\": "tests"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
