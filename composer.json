{
    "name": "medcenter24/mc-core",
    "description": "Backend for the MedCenter24 project",
    "keywords": ["api", "medcenter24", "backoffice", "medicine"],
    "license": "GPL-2.0-only",
    "minimum-stability": "dev",
    "type": "medcenter24-extension",
    "prefer-stable": true,
    "extra": {
        "medcenter24-extension-name": "mcCore",
        "laravel": {
            "dont-discover": []
        }
    },
    "require": {
        "php": ">=7.2",
        "ext-json": "*",
        "ext-libxml":  "*",
        "ext-dom": "*",
        "backup-manager/laravel": "1.4.*",
        "barryvdh/laravel-cors": "0.11.4",
        "cmgmyr/messenger": "^2.19",
        "dingo/api": "~2.4.0",
        "intervention/image": "^2.5",
        "irazasyed/telegram-bot-sdk": "3.0.*",
        "fideloper/proxy": "^4.2",
        "laravel/framework": "^6.0",
        "laravel/tinker": "~2.0",
        "maatwebsite/excel": "^3.1.17",
        "mpdf/mpdf": "~8.0.0",
        "medcenter24/lib-installer": "~1.0",
        "spatie/laravel-medialibrary": "~7.14.2",
        "srmklive/flysystem-dropbox-v2": "1.0.7",
        "tymon/jwt-auth": "1.0.0-rc.5",
        "nwidart/laravel-modules": "6.2.*"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "~3.2.8",
        "barryvdh/laravel-ide-helper": "^2.6.5",
        "filp/whoops": "^2.5.0",
        "doctrine/dbal": "^2.10.0",
        "fzaninotto/faker": "~1.9.0",
        "mockery/mockery": "1.3.*",
        "nunomaduro/collision": "^3.0.1",
        "phpunit/phpunit": "~8.4.3",
        "roave/security-advisories": "dev-master"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "medcenter24\\mcCore\\App\\": "app",
            "medcenter24\\mcCore\\Tests\\": "tests"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "medcenter24\\mcCore\\Tests\\": "tests"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}
