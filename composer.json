{
    "name": "medcenter24/md-core",
    "description": "Backend for the MedCenter24 project",
    "keywords": ["api", "medcenter24", "backoffice", "medicine"],
    "license": "GPL-2.0-only",
    "minimum-stability": "dev",
    "type": "medcenter24-extension",
    "extra": {
        "medcenter24-extension-name": "mcCore"
    },
    "require": {
        "ext-json": "*",
        "php": ">=7.1.3",
        "backup-manager/laravel": "dev-master",
        "barryvdh/laravel-cors": "0.11.3",
        "cmgmyr/messenger": "dev-master",
        "dingo/api": "~2.2.2",
        "intervention/image": "^2.4",
        "irazasyed/telegram-bot-sdk": "3.0.*",
        "laravel/framework": "5.8.*",
        "laravel/tinker": "~1.0",
        "maatwebsite/excel": "~3.1.10",
        "mpdf/mpdf": "~8.0.0",
        "medcenter24/lib-installer": "~1.0",
        "spatie/laravel-medialibrary": "~7.6.0",
        "srmklive/flysystem-dropbox-v2": "dev-master",
        "tymon/jwt-auth": "1.0.0-rc.4.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "~3.2.2",
        "barryvdh/laravel-ide-helper": "^2.5",
        "doctrine/dbal": "^2.9.2",
        "fzaninotto/faker": "~1.8",
        "mockery/mockery": "1.2.*",
        "nunomaduro/collision": "^3.0.1",
        "phpunit/phpunit": "~7.5"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
