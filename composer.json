{
    "name": "mydoctors24/backend-laravel",
    "description": "Backend for the myDoctors 24 project, based on Laravel framework",
    "keywords": ["api", "mydoctors24"],
    "license": "GNU-2",
    "minimum-stability": "dev",
    "type": "mydoctors24-extension",
    "extra": {
        "mydoctors24-extension-name": "myDoctors24"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@bitbucket.org:mydoctor24/lib-installer.git"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "barryvdh/laravel-cors": "^0.10.0",
        "dingo/api": "2.0.0-alpha1",
        "intervention/image": "^2.4",
        "laravel/framework": "5.5.*",
        "laravel/tinker": "~1.0",
        "maatwebsite/excel": "~2.1.0",
        "mpdf/mpdf": "dev-master",
        "mydoctors24/lib-installer": "~0.0.1",
        "spatie/laravel-medialibrary": "^6.6.3",
        "tymon/jwt-auth": "^1.0.0-rc.1"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.4",
        "fzaninotto/faker": "~1.7",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
