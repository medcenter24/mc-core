{
    "name": "medcenter24/md-core",
    "description": "Backend for the MedCenter24 project",
    "keywords": ["api", "mydoctors24", "medcenter24", "backoffice", "medicine"],
    "license": "GPL-2.0-only",
    "minimum-stability": "dev",
    "type": "mydoctors24-extension",
    "extra": {
        "mydoctors24-extension-name": "mdCore"
    },
    "require": {
        "ext-json": "*",
        "php": ">=7.1.3",
        "backup-manager/laravel": "dev-master",
        "barryvdh/laravel-cors": "0.11.2",
        "cmgmyr/messenger": "dev-master",
        "dingo/api": "~2.0.1",
        "fideloper/proxy": "~4.1",
        "intervention/image": "^2.4",
        "irazasyed/telegram-bot-sdk": "3.0.*",
        "laravel/framework": "5.7.*",
        "laravel/tinker": "~1.0",
        "maatwebsite/excel": "~3.1.7",
        "mpdf/mpdf": "~7.1.9",
        "medcenter24/lib-installer": "~0.1",
        "phpdocumentor/reflection-common": "1.0",
        "spatie/laravel-medialibrary": "~7.5.5",
        "srmklive/flysystem-dropbox-v2": "dev-master",
        "tymon/jwt-auth": "1.0.0-rc.3"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "~3.2.2",
        "barryvdh/laravel-ide-helper": "^2.5",
        "doctrine/dbal": "^2.9.2",
        "fzaninotto/faker": "~1.8",
        "mockery/mockery": "1.2.*",
        "nunomaduro/collision": "~2.1",
        "phpunit/phpunit": "~7.4",
        "nwidart/laravel-modules": "^4.0@dev"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "Modules/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
